{
  "format_version": "1.19.0",
  "animation_controllers": {
    "controller.animation.re_dr.dragon_all.dragon": {
      "initial_state": "idle_ground",
      "states": {
        "idle_ground": {
          "animations": ["idle_ground"],
          "transitions": [
            {
              "fly": "q.is_jumping || (!q.has_player_rider && q.vertical_speed >= 0 && !q.is_jumping && v.time_since_ground>0.25)"
            },
            {
              "fly": "q.is_jumping ||( q.has_player_rider && q.vertical_speed >= 0 && !q.is_jumping && v.time_since_ground>0.25)"
            },
            {
              "walk": "(q.is_on_ground && q.modified_move_speed > 0.125) && !q.is_in_water"
            }
          ],
          "blend_transition": 0.2,
          "blend_via_shortest_path": true
        },
        "walk": {
          "animations": ["walk"],
          "transitions": [
            {
              "fly": "q.is_jumping ||( q.has_player_rider && q.vertical_speed >= 0 && !q.is_jumping && v.time_since_ground>0.25)"
            },
            {
              "idle_ground": "(q.is_on_ground && q.modified_move_speed <= 0.125) && !q.is_in_water"
            }
          ],
          "blend_transition": 0.2,
          "blend_via_shortest_path": true
        },
        "fly": {
          "animations": ["flying"],
          "transitions": [
            {
              "idle_ground": "(q.is_on_ground && q.modified_move_speed <= 0.125) && !q.is_in_water"
            },
            {
              "walk": "(q.is_on_ground && q.modified_move_speed > 0.125) && !q.is_in_water"
            },
            {
              "fly_hover": "(!q.is_on_ground && q.vertical_speed <= 0.015 && q.modified_move_speed <= 0.1) && !q.is_in_water"
            }
          ],
          "blend_transition": 0.2,
          "blend_via_shortest_path": true
        },
        "fly_hover": {
          "animations": ["idle_fly"],
          "transitions": [
            {
              "idle_ground": "(q.is_on_ground && q.modified_move_speed <= 0.125) && !q.is_in_water"
            },
            {
              "walk": "(q.is_on_ground && q.modified_move_speed > 0.125) && !q.is_in_water"
            },
            {
              "fly": "(!q.is_on_ground && q.vertical_speed > 0.015 && q.modified_move_speed > 0.1) && !q.is_in_water"
            }
          ],
          "blend_transition": 0.2,
          "blend_via_shortest_path": true
        }
      }
    },
    "controller.animation.re_dr.dragon_all.dragon_baby": {
      "initial_state": "idle_ground",
      "states": {
        "idle_ground": {
          "animations": ["idle_ground"],
          "transitions": [
            {
              "walk": "(q.is_on_ground && q.modified_move_speed > 0.125) && !q.is_in_water"
            }
          ],
          "blend_transition": 0.2,
          "blend_via_shortest_path": true
        },
        "walk": {
          "animations": ["walk"],
          "transitions": [
            {
              "idle_ground": "(q.is_on_ground && q.modified_move_speed <= 0.125) && !q.is_in_water"
            }
          ],
          "blend_transition": 0.2,
          "blend_via_shortest_path": true
        }
      }
    },
    "controller.animation.re_dr.dragon_all.dragon_update": {
      "initial_state": "idle_ground",
      "states": {
        "idle_ground": {
          "animations": ["idle_ground"],
          "transitions": [
            {
              "airborne": "q.property('re_dra:flight_state') == 'airborne' && !q.is_in_water"
            },
            {
              "walk": "(q.is_on_ground && q.modified_move_speed > 0.125) && !q.is_in_water && q.property('re_dra:flight_state') == 'grounded'"
            }
          ],
          "blend_transition": 0.2,
          "blend_via_shortest_path": true
        },
        "walk": {
          "animations": ["walk"],
          "transitions": [
            {
              "airborne": "q.property('re_dra:flight_state') == 'airborne' && !q.is_in_water"
            },
            {
              "idle_ground": "(q.is_on_ground && q.modified_move_speed <= 0.125) && !q.is_in_water && q.property('re_dra:flight_state') == 'grounded'"
            }
          ],
          "blend_transition": 0.2,
          "blend_via_shortest_path": true
        },
        "airborne": {
          "animations": ["ground_to_fly"],
          "transitions": [
            {
              "fly_hover": "q.property('re_dra:flight_state') == 'flying' && q.any_animation_finished"
            }
          ],
          "blend_transition": 0.2,
          "blend_via_shortest_path": true
        },
        "flying": {
          "animations": ["flying"],
          "transitions": [
            {
              "landing": "q.property('re_dra:flight_state') == 'landing' && !q.is_in_water"
            },
            {
              "fly_hover": "(q.property('re_dra:flight_state') == 'flying' && q.modified_move_speed <= 0.1) && !q.is_in_water"
            }
          ],
          "blend_transition": 0.2,
          "blend_via_shortest_path": true
        },
        "landing": {
          "animations": ["fly_to_ground"],
          "transitions": [
            {
              "idle_ground": "q.property('re_dra:flight_state') == 'grounded' && !q.is_in_water && q.any_animation_finished"
            },
            {
              "fly_hover": "q.property('re_dra:flight_state') == 'flying' && !q.is_in_water && q.any_animation_finished"
            }
          ],
          "blend_transition": 0.2,
          "blend_via_shortest_path": true
        },
        "fly_hover": {
          "animations": ["idle_fly"],
          "transitions": [
            {
              "landing": "q.property('re_dra:flight_state') == 'landing' && !q.is_in_water"
            },
            {
              "flying": "(q.property('re_dra:flight_state') == 'flying' && q.modified_move_speed > 0.1) && !q.is_in_water"
            }
          ],
          "blend_transition": 0.2,
          "blend_via_shortest_path": true
        }
      }
    }
  }
}
