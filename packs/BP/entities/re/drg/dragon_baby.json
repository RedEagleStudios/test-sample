{
  "format_version": "1.21.120",
  "minecraft:entity": {
    "description": {
      "identifier": "re_dra:dragon_baby",
      "spawn_category": "creature",
      "is_spawnable": true,
      "is_summonable": true,
      "properties": {
        "minecraft:can_move": {
          "type": "bool",
          "default": true,
          "client_sync": true
        },
        "re_furn:friendship": {
          "client_sync": true,
          "default": 0,
          "type": "int",
          "range": [0, 3]
        },
        "re_dra:flight_state": {
          "type": "enum",
          "default": "grounded",
          "client_sync": true,
          "values": ["grounded", "hovering", "flying"]
        },
        "re_dra:want": {
          "type": "enum",
          "default": "none",
          "client_sync": true,
          "values": [
            "none",
            "hungry",
            "playful",
            "hungry_success",
            "hungry_success_favorite",
            "playful_success",
            "saddle",
            "saddle_success"
          ]
        },
        "re_dra:rest_state": {
          "type": "enum",
          "default": "none",
          "client_sync": true,
          "values": ["none", "sitting", "sleeping", "scratching"]
        },
        "re_dra:is_flying": {
          "type": "bool",
          "default": false,
          "client_sync": true
        },
        "re_dra:is_hovering": {
          "type": "bool",
          "default": false,
          "client_sync": true
        },
        "re_dra:is_pregnant": {
          "type": "bool",
          "default": false,
          "client_sync": true
        },
        "re_dra:is_melee_attacking": {
          "type": "bool",
          "default": false,
          "client_sync": true
        },
        "re_dra:is_ranged_attacking": {
          "type": "bool",
          "default": false,
          "client_sync": true
        },
        "re_dra:is_saddled": {
