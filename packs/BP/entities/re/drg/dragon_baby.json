        "re_dra:is_boosting": {
          "type": "bool",
          "default": false,
          "client_sync": true
        },
        "re_dra:has_armor": {
          "type": "bool",
          "default": false,
          "client_sync": true
        },
        "re_dra:is_tamed": {
          "type": "bool",
          "default": false,
          "client_sync": true
        }
      }
    },
