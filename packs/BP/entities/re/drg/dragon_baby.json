    "events": {
      "minecraft:entity_spawned": {
        "add": {
          "component_groups": ["re_dra:baby"]
        }
      },
      "re_dra:event_grow_up_to_teen": {
        "add": {
          "component_groups": ["re_dra:teen", "re_dra:teen_roam"]
        },
        "remove": {
          "component_groups": ["re_dra:baby"]
        }
      },
      "re_dra:event_grow_up_to_adult": {
        "add": {
          "component_groups": ["re_dra:transform_to_adult"]
        },
        "remove": {
          "component_groups": ["re_dra:teen","re_dra:teen_roam","re_dra:teen_follow", "re_dra:teen_sit"]
        }
      },
      /*START OF TEEN EVENTS*/
      "re_dra:event_teen_roam": {
